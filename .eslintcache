pdatedPrice = totalPrice - product.price;\r\n                updatedQty = qty - 1;\r\n                index = shoppingCart.findIndex(cart => cart.id === action.id);\r\n                shoppingCart[index] = product;\r\n                return { shoppingCart: [...shoppingCart], totalPrice: updatedPrice, qty: updatedQty }\r\n            }\r\n            else {\r\n                return state;\r\n            }\r\n            break;\r\n\r\n        case 'DELETE':\r\n            const filtered = shoppingCart.filter(product => product.id !== action.id)\r\n            product = action.cart;\r\n            updatedQty = qty - product.qty;\r\n            updatedPrice = totalPrice - product.price * product.qty;\r\n            return { shoppingCart: [...filtered], totalPrice: updatedPrice, qty: updatedQty }\r\n            break;\r\n\r\n        case 'EMPTY':\r\n            return { shoppingCart: [], totalPrice: 0, qty: 0 }\r\n\r\n        default:\r\n            return state\r\n\r\n    }\r\n\r\n}\r\n",{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","severity":1,"message":"89","line":2,"column":8,"nodeType":"90","messageId":"91","endLine":2,"endColumn":14},{"ruleId":"84","replacedBy":"92"},{"ruleId":"86","replacedBy":"93"},{"ruleId":"88","severity":1,"message":"89","line":2,"column":8,"nodeType":"90","messageId":"91","endLine":2,"endColumn":14},{"ruleId":"88","severity":1,"message":"94","line":5,"column":10,"nodeType":"90","messageId":"91","endLine":5,"endColumn":24},{"ruleId":"95","severity":1,"message":"96","line":10,"column":67,"nodeType":"97","messageId":"98","endLine":10,"endColumn":69},{"ruleId":"99","severity":1,"message":"100","line":20,"column":13,"nodeType":"101","messageId":"102","endLine":20,"endColumn":19},{"ruleId":"99","severity":1,"message":"100","line":31,"column":13,"nodeType":"101","messageId":"102","endLine":31,"endColumn":19},{"ruleId":"99","severity":1,"message":"100","line":46,"column":13,"nodeType":"101","messageId":"102","en